int threshold;
int frame = 0;

void setup() {
  Serial.begin(115200);
  u8g2.begin();
  delay(2000);
  threshold = touchRead(T5) - 70;
  Serial.println("Touch threshold set");
}

void playFrames(int start, int end, int delayMs, bool forward = true) {
  if (forward) {
    for (int i = start; i <= end; i++) {
      u8g2.clearBuffer();
      u8g2.drawXBMP(0, 0, 128, 64, epd_bitmap_allArray[i]);
      u8g2.sendBuffer();
      Serial.print("Frame: "); Serial.println(i);
      delay(delayMs);
    }
  } else {
    for (int i = start; i >= end; i--) {
      u8g2.clearBuffer();
      u8g2.drawXBMP(0, 0, 128, 64, epd_bitmap_allArray[i]);
      u8g2.sendBuffer();
      Serial.print("Frame: "); Serial.println(i);
      delay(delayMs);
    }
  }
}

void loop() {

  while (touchRead(T5) >= threshold) {
    playFrames(0, 7, 2, true);

    unsigned long pauseStart = millis();
    while (millis() - pauseStart < 1500) {
      if (touchRead(T5) < threshold) break;
      delay(10);
    }

    if (touchRead(T5) < threshold) break;
  }


  playFrames(8, 22, 15, true);
  while (touchRead(T5) < threshold) {
    delay(100);
  }

  playFrames(22, 8, 15, false);

  unsigned long pauseStart2 = millis();
  while (millis() - pauseStart2 < 1500) {
    if (touchRead(T5) < threshold) break;
    delay(10);
  }
}
